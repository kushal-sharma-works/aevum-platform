{
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": {
          "type": "grafana",
          "uid": "-- Grafana --"
        },
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 0,
  "id": null,
  "links": [],
  "panels": [
    {
      "type": "timeseries",
      "title": "Ingestion Rate by Stream",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 0
      },
      "targets": [
        {
          "expr": "sum by(stream_id) (rate(aevum_events_ingested_total[1m]))",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Ingestion Latency Histogram",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 0
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, rate(aevum_ingestion_duration_seconds_bucket[5m]))",
          "refId": "A"
        }
      ]
    },
    {
      "type": "gauge",
      "title": "Active Replays",
      "gridPos": {
        "h": 6,
        "w": 8,
        "x": 0,
        "y": 8
      },
      "targets": [
        {
          "expr": "sum(aevum_replays_active)",
          "refId": "A"
        }
      ]
    },
    {
      "type": "piechart",
      "title": "Events by Type",
      "gridPos": {
        "h": 6,
        "w": 8,
        "x": 8,
        "y": 8
      },
      "targets": [
        {
          "expr": "sum by(event_type) (increase(aevum_events_ingested_total[5m]))",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "HTTP Request Rate / Errors by Endpoint",
      "gridPos": {
        "h": 6,
        "w": 8,
        "x": 16,
        "y": 8
      },
      "targets": [
        {
          "expr": "sum by(path) (rate(http_server_requests_total[1m]))",
          "refId": "A"
        },
        {
          "expr": "sum by(path) (rate(http_server_requests_total{status=~\"5..\"}[1m]))",
          "refId": "B"
        }
      ]
    }
  ],
  "refresh": "30s",
  "schemaVersion": 39,
  "style": "dark",
  "tags": [
    "aevum"
  ],
  "templating": {
    "list": [
      {
        "name": "namespace",
        "label": "Namespace",
        "type": "query",
        "datasource": {
          "type": "prometheus",
          "uid": "${DS_PROMETHEUS}"
        },
        "query": "label_values(kube_pod_info, namespace)",
        "includeAll": true,
        "multi": false,
        "current": {
          "selected": true,
          "text": "All",
          "value": "$__all"
        }
      }
    ]
  },
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "timepicker": {},
  "timezone": "",
  "title": "Event Timeline Service",
  "uid": null,
  "version": 1,
  "weekStart": ""
}