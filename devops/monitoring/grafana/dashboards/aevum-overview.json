{
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": {
          "type": "grafana",
          "uid": "-- Grafana --"
        },
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 0,
  "id": null,
  "links": [],
  "panels": [
    {
      "type": "stat",
      "title": "Total Events Ingested",
      "gridPos": {
        "h": 5,
        "w": 6,
        "x": 0,
        "y": 0
      },
      "targets": [
        {
          "expr": "sum(aevum_events_ingested_total)",
          "refId": "A"
        }
      ]
    },
    {
      "type": "stat",
      "title": "Total Decisions Made",
      "gridPos": {
        "h": 5,
        "w": 6,
        "x": 6,
        "y": 0
      },
      "targets": [
        {
          "expr": "sum(aevum_decisions_evaluated_total)",
          "refId": "A"
        }
      ]
    },
    {
      "type": "gauge",
      "title": "Active Rules",
      "gridPos": {
        "h": 5,
        "w": 6,
        "x": 12,
        "y": 0
      },
      "targets": [
        {
          "expr": "sum(aevum_active_rules)",
          "refId": "A"
        }
      ]
    },
    {
      "type": "stat",
      "title": "System Error Rate",
      "gridPos": {
        "h": 5,
        "w": 6,
        "x": 18,
        "y": 0
      },
      "targets": [
        {
          "expr": "aevum:error_rate:5m",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Ingestion Rate",
      "gridPos": {
        "h": 7,
        "w": 12,
        "x": 0,
        "y": 5
      },
      "targets": [
        {
          "expr": "aevum:ingestion_rate:1m",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Ingestion Latency P50/P95/P99",
      "gridPos": {
        "h": 7,
        "w": 12,
        "x": 12,
        "y": 5
      },
      "targets": [
        {
          "expr": "aevum:ingestion_latency_p50:5m",
          "refId": "A"
        },
        {
          "expr": "aevum:ingestion_latency_p95:5m",
          "refId": "B"
        },
        {
          "expr": "aevum:ingestion_latency_p99:5m",
          "refId": "C"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Decision Rate",
      "gridPos": {
        "h": 7,
        "w": 12,
        "x": 0,
        "y": 12
      },
      "targets": [
        {
          "expr": "aevum:decision_rate:1m",
          "refId": "A"
        }
      ]
    },
    {
      "type": "barchart",
      "title": "Decision Status Distribution",
      "gridPos": {
        "h": 7,
        "w": 12,
        "x": 12,
        "y": 12
      },
      "targets": [
        {
          "expr": "sum by(status) (increase(aevum_decisions_evaluated_total[5m]))",
          "refId": "A"
        }
      ]
    },
    {
      "type": "gauge",
      "title": "Search Sync Lag",
      "gridPos": {
        "h": 7,
        "w": 8,
        "x": 0,
        "y": 19
      },
      "targets": [
        {
          "expr": "max(aevum_sync_lag_seconds)",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Search Query Duration",
      "gridPos": {
        "h": 7,
        "w": 16,
        "x": 8,
        "y": 19
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, rate(aevum_query_duration_seconds_bucket[5m]))",
          "refId": "A"
        }
      ]
    }
  ],
  "refresh": "30s",
  "schemaVersion": 39,
  "style": "dark",
  "tags": [
    "aevum"
  ],
  "templating": {
    "list": [
      {
        "name": "namespace",
        "label": "Namespace",
        "type": "query",
        "datasource": {
          "type": "prometheus",
          "uid": "${DS_PROMETHEUS}"
        },
        "query": "label_values(kube_pod_info, namespace)",
        "includeAll": true,
        "multi": false,
        "current": {
          "selected": true,
          "text": "All",
          "value": "$__all"
        }
      }
    ]
  },
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "timepicker": {},
  "timezone": "",
  "title": "Aevum Overview",
  "uid": null,
  "version": 1,
  "weekStart": ""
}