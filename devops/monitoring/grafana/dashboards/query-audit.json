{
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": {
          "type": "grafana",
          "uid": "-- Grafana --"
        },
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 0,
  "id": null,
  "links": [],
  "panels": [
    {
      "type": "timeseries",
      "title": "Search Query Rate",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 0
      },
      "targets": [
        {
          "expr": "rate(aevum_search_queries_total[1m])",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Query Latency",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 0
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, rate(aevum_query_duration_seconds_bucket[5m]))",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Index Operations Rate",
      "gridPos": {
        "h": 7,
        "w": 8,
        "x": 0,
        "y": 8
      },
      "targets": [
        {
          "expr": "rate(aevum_index_ops_total[1m])",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Sync Lag per Source",
      "gridPos": {
        "h": 7,
        "w": 8,
        "x": 8,
        "y": 8
      },
      "targets": [
        {
          "expr": "max by(source) (aevum_sync_lag_seconds)",
          "refId": "A"
        }
      ]
    },
    {
      "type": "stat",
      "title": "Index Size",
      "gridPos": {
        "h": 7,
        "w": 8,
        "x": 16,
        "y": 8
      },
      "targets": [
        {
          "expr": "sum(aevum_index_size_bytes)",
          "refId": "A"
        }
      ]
    }
  ],
  "refresh": "30s",
  "schemaVersion": 39,
  "style": "dark",
  "tags": [
    "aevum"
  ],
  "templating": {
    "list": [
      {
        "name": "namespace",
        "label": "Namespace",
        "type": "query",
        "datasource": {
          "type": "prometheus",
          "uid": "${DS_PROMETHEUS}"
        },
        "query": "label_values(kube_pod_info, namespace)",
        "includeAll": true,
        "multi": false,
        "current": {
          "selected": true,
          "text": "All",
          "value": "$__all"
        }
      }
    ]
  },
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "timepicker": {},
  "timezone": "",
  "title": "Query & Audit Service",
  "uid": null,
  "version": 1,
  "weekStart": ""
}